{% block navbar %}

    <div class="sticky">
        <nav data-options="sticky_on: large" role="navigation" data-topbar="" class="top-bar">
            <ul class="title-area">
                <li class="name">
                    <span class="title">
                        <a href="#">
                            <span class="icon-medium icon-home vertical-middle"></span>
                            {{ 'views.navbar.title'|trans }}
                        </a>
                    </span>
                </li>
                <li class="toggle-topbar menu-icon">
                    <a href="#">
                        <span>{{ 'views.navbar.menu'|trans }}</span>
                    </a>
                </li>
            </ul>
            <section class="top-bar-section">
                <ul class="right">
                    <li class="divider hide-for-small"></li>
                    <li>
                        <a class="" href="{{ path('zimzim_categoryproduct_category_listid', {'id':1}) }}">
                            <span class="icon-medium icon-file vertical-middle"></span>
                            {{ 'views.navbar.product'|trans }}
                        </a>
                    </li>
                    <li>
                        <a class="" href="#">
                            <span class="icon-medium icon-address-book vertical-middle"></span>
                            {{ 'views.navbar.contact'|trans }}
                        </a>
                    </li>
                </ul>
            </section>
        </nav>
    </div>


{% endblock navbar %}